{
  "resourceType": "StructureDefinition",
  "id": "HaPatient",
  "text": {
    "status": "additional",
    "div": "<div xmlns=\"http://www.w3.org/1999/xhtml\">healthAlliance Patient profile</div>"
  },
  "url": "http://hl7.org.nz/healthalliance/StructureDefinition/HaPatient",
  "version": "0.1.0",
  "name": "HaPatient",
  "title": "Common Patient profile",
  "status": "active",
  "description": "Represents patient demographics exposed by healthAlliance systems",
  "fhirVersion": "4.0.1",
  "mapping": [
    {
      "identity": "rim",
      "uri": "http://hl7.org/v3",
      "name": "RIM Mapping"
    },
    {
      "identity": "cda",
      "uri": "http://hl7.org/v3/cda",
      "name": "CDA (R2)"
    },
    {
      "identity": "w5",
      "uri": "http://hl7.org/fhir/fivews",
      "name": "FiveWs Pattern Mapping"
    },
    {
      "identity": "v2",
      "uri": "http://hl7.org/v2",
      "name": "HL7 v2 Mapping"
    },
    {
      "identity": "loinc",
      "uri": "http://loinc.org",
      "name": "LOINC code for the element"
    }
  ],
  "kind": "resource",
  "abstract": false,
  "type": "Patient",
  "baseDefinition": "http://hl7.org/fhir/StructureDefinition/Patient",
  "derivation": "constraint",
  "differential": {
    "element": [
      {
        "id": "Patient.identifier",
        "path": "Patient.identifier",
        "slicing": {
          "discriminator": [
            {
              "type": "value",
              "path": "use"
            }
          ],
          "rules": "openAtEnd"
        }
      },
      {
        "id": "Patient.identifier.type",
        "path": "Patient.identifier.type",
        "max": "0"
      },
      {
        "id": "Patient.identifier.period",
        "path": "Patient.identifier.period",
        "max": "0"
      },
      {
        "id": "Patient.identifier.assigner",
        "path": "Patient.identifier.assigner",
        "max": "0"
      },
      {
        "id": "Patient.identifier:NHI",
        "path": "Patient.identifier",
        "sliceName": "NHI",
        "max": "1",
        "mustSupport": true
      },
      {
        "id": "Patient.identifier:NHI.use",
        "path": "Patient.identifier.use",
        "min": 1,
        "fixedCode": "official"
      },
      {
        "id": "Patient.identifier:NHI.system",
        "path": "Patient.identifier.system",
        "patternUri": "https://standards.digital.health.nz/id/nhi"
      },
      {
        "id": "Patient.active",
        "path": "Patient.active",
        "max": "0"
      },
      {
        "id": "Patient.name",
        "path": "Patient.name",
        "min": 1
      },
      {
        "id": "Patient.name.extension:preferred",
        "path": "Patient.name.extension",
        "sliceName": "preferred",
        "max": "1",
        "type": [
          {
            "code": "Extension",
            "profile": [
              "http://hl7.org/fhir/StructureDefinition/iso21090-preferred"
            ]
          }
        ]
      },
      {
        "id": "Patient.address",
        "path": "Patient.address",
        "min": 1
      },
      {
        "id": "Patient.address.extension:suburb",
        "path": "Patient.address.extension",
        "sliceName": "suburb",
        "max": "1",
        "type": [
          {
            "code": "Extension",
            "profile": [
              "http://hl7.org.nz/fhir/StructureDefinition/suburb"
            ]
          }
        ]
      },
      {
        "id": "Patient.address.line",
        "path": "Patient.address.line",
        "min": 1
      },
      {
        "id": "Patient.maritalStatus",
        "path": "Patient.maritalStatus",
        "max": "0"
      },
      {
        "id": "Patient.multipleBirth[x]",
        "path": "Patient.multipleBirth[x]",
        "max": "0"
      },
      {
        "id": "Patient.photo",
        "path": "Patient.photo",
        "max": "0"
      },
      {
        "id": "Patient.contact",
        "path": "Patient.contact",
        "max": "0"
      },
      {
        "id": "Patient.communication",
        "path": "Patient.communication",
        "max": "0"
      },
      {
        "id": "Patient.generalPractitioner",
        "path": "Patient.generalPractitioner",
        "type": [
          {
            "code": "Reference",
            "targetProfile": [
              "http://hl7.org.nz/healthalliance/StructureDefinition/HaPractitioner",
              "http://hl7.org.nz/healthalliance/StructureDefinition/HaOrganization"
            ]
          }
        ]
      },
      {
        "id": "Patient.managingOrganization",
        "path": "Patient.managingOrganization",
        "type": [
          {
            "code": "Reference",
            "targetProfile": [
              "http://hl7.org.nz/healthalliance/StructureDefinition/HaDhb"
            ]
          }
        ]
      },
      {
        "id": "Patient.link",
        "path": "Patient.link",
        "max": "0"
      }
    ]
  }
}
